pipeline{
		agent any 

		stages{
			stage('Complile stage') {
				steps{
					withMaven(maven : 'maven 3.3.9'){
					sh 'mvn clean compile'
						}
					}
				}
				
			stage('Testing stage') {
			steps{
					withMaven(maven : 'maven 3.3.9'){
					sh 'mvn test'
						}
					}
				}
				
					
			stage('Deployment stage') {
			steps{
					withMaven(maven : 'maven 3.3.9'){
					sh 'mvn deploy'
						}
					}
				}
			} 	
	}